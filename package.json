  "description": "validation library for spinajs framework",
  "main": "lib/index.js",
  "private": false,
  "scripts": {
    "test": "ts-mocha -p tsconfig.json test/**/*.test.ts",
    "coverage": "nyc npm run test",
    "build-docs": "rimraf docs && typedoc --options typedoc.json src/",
    "build": "tsc",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepublishOnly": "",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spinajs/validation.git"
  },
  "keywords": [
    "spinajs",
    "validation",
    "schema"
  ],
  "author": "SpinaJS <spinajs@coderush.pl> (https://github.com/spinajs/core)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spinajs/validation/issues"
  },
  "homepage": "https://github.com/spinajs/validation#readme",
  "dependencies": {
    "@spinajs/configuration": "^1.0.16",
    "@spinajs/di": "^1.1.0",
    "@spinajs/exceptions": "^1.0.4",
    "@spinajs/log": "^1.0.10",
    "ajv": "^6.12.2",
    "ajv-formats": "^2.1.1",
    "ajv-keywords": "^5.1.0",
    "ajv-merge-patch": "^4.1.0"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.6",
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cookie-signature": "^1.0.3",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/formidable": "^1.0.31",
    "@types/helmet": "^0.0.47",
    "@types/lodash": "^4.14.136",
    "@types/luxon": "^2.0.8",
    "@types/mime": "^2.0.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^16.11.11",
    "@types/pug": "^2.0.4",
    "@types/randomstring": "^1.1.6",
    "@types/sinon": "^7.0.13",
    "@types/uuid": "^8.3.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-dom": "^1.8.2",
    "chai-http": "^4.3.0",
    "cpx": "^1.5.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "sinon": "^7.3.2",
    "ts-mocha": "^6.0.0",
    "ts-node": "^8.3.0",
    "tslint": "^5.20.1",
    "tslint-circular-dependencies": "^0.1.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-no-unused-expression-chai": "^0.1.4",
    "typedoc": "^0.14.2",
    "typescript": "^3.7.3"
